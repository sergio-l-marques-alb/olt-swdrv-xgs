#!/bin/sh

CMD_MAX_ARGS=25

# $# minus 1 because of $0
let "insrtd_args = $# - 1"

if [ "$insrtd_args" -gt "$CMD_MAX_ARGS" ]
then
    echo "Please check the code for CMD_MAX_ARGS (currently $CMD_MAX_ARGS)!"
